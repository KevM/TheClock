{>"layouts/master" /}

{<body}
<h1>{room.location} <small>{room.name}</small></h1>

<h2>Present in room</h2>
{?users}
  <ul>
    {#users}
    <li>
        {name} - checked in at {createdAt}
    </li>
    {/users}
  </ul>
{:else}
<div class="alert alert-info">
    The room is currently empty.
</div>
{/users}
{/body}
